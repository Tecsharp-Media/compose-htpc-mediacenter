# Global settings
	# Timezone
  TIMEZONE="America/Toronto"
	# User ID of bind mount
  PUID="1000"
	# Group ID of bind mount
  PGID="1000"

# Directory mounts
	# Location of appdata root
  APPDATA="/data/appdata/mediacenter"
	# Location of media mount
  MEDIA="/data/media"
	# Location of transcoding mount
  TRANSCODING="/data/transcoding"
	# Location of usenet download directory
  USENET_DOWNLOADS="/data/downloads"
	# Location of torrent download directory
  TORRENT_DOWNLOADS="/data/local_media/downloads"

# Domain names
	# Root domain name
  DOMAIN=""
	# Admin domain
	ADMIN_DOMAIN="admin"
	# Media domain
	MEDIA_DOMAIN="media"

# Frontend domain names
	# Organizr domain
	ORGANIZR_DOMAIN="web"
