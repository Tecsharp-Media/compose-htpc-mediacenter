	# Subdomain name of ombi
	OMBI_DOMAIN="ombi"
	# Subdomain name of tautulli
	TAUTULLI_DOMAIN="metrics"

# Indexer domain names
	# Subdomain name of sonarr
	SONARR_DOMAIN="tv"
	# Subdomain name of radarr
	RADARR_DOMAIN="movies"
	# Subdomain name of jackett
	JACKETT_DOMAIN="indexer"
	# Subdomain name of hydra2
	HYDRA2_DOMAIN="search"

# Downloader domain names
